{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "lint": "eslint --ext .js,.ts src"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^7.10.3",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-plugin-jsx-control-statements": "^4.1.2",
    "eslint": "^8.38.0",
    "jsencrypt": "^3.3.2",
    "node-rsa": "^1.1.1",
    "qs": "^6.11.1",
    "react": "17.x",
    "react-dom": "17.x",
    "tsx-control-statements": "^5.1.0",
    "umi": "^3.5.40"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.40",
    "ant-design": "^1.0.0",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.21",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
